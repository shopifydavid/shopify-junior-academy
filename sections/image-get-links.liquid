<div class="page-width">
  <h2>
  Using snippets
  </h2>
  <div style="border: 1ps solid orange;">
    {% render 'icons', icon: 'tiktok' %}
  </div>
  <h3>
    | img_url: '500' - old version
    <br>
    | image_url: width: 500  - new version
  </h3>
  {% if section.settings.get_image_v_1 != blank %}
    <div id="get_image_v_1" style="padding: 5px; margin: 5px; border: 1px solid black;">
      <h2>get_image_v_1</h2>
      <div style="border: 1px linear blue;">
        section.settings.get_image_v_1 == {{ section.settings.get_image_v_1  }}
        <br>
        section.settings.get_image_v_1 | img_url: 'icon' == {{ section.settings.get_image_v_1  | img_url: 'icon' }}
        <br>
        section.settings.get_image_v_1 | img_url: '33x' == {{ section.settings.get_image_v_1  | img_url: '33x' }}
        <br>
        section.settings.get_image_v_1 | img_url: '33x' | img_tag == {{ section.settings.get_image_v_1  | img_url: '33x' | img_tag  }}
      </div>
      <div style="border: 1px solid green;">
        <img src="{{section.settings.get_image_v_1 | img_url: 'icon'}}" alt="{{ section.settings.get_image_v_1.alt }}">
        <br>
        <img src="{{section.settings.get_image_v_1 | img_url: '500'}}" alt="{{ section.settings.get_image_v_1.alt }}">
        <br>
        <img src="{{section.settings.get_image_v_1 | img_url: 'large'}}" alt="{{ section.settings.get_image_v_1.alt }}">
        <br>
        {{ section.settings.get_image_v_1 | img_url: '510x' | img_tag }}
        <br>
        {{ section.settings.get_image_v_1 | image_url: width: 500 | image_tag: class: 'custom-class', loading: 'lazy' }}
      </div>
    </div> 
  {% endif %}   
    <div id="get_image_v_2" style="padding: 5px; margin: 5px; border: 1px solid black;">
      {% if section.settings.get_image_v_2 != blank %}
        <h3>Have images</h3>
      {% else %}
        <h3>Haven`t image</h3>
      {% endif %}
      <h2>get_image_v_2</h2>
      section.settings.get_image_v_2 == {{ section.settings.get_image_v_2  }}
      <br>
      section.settings.get_image_v_2 | img_url: 'icon' == {{ section.settings.get_image_v_2  | img_url: 'icon' }}
      <br>
      section.settings.get_image_v_2 | img_url: '33x' == {{ section.settings.get_image_v_2  | img_url: '33x' }}
    </div>
    <div id="get_image_v_3">
      <h2>get_image_v_3 - assets</h2>
      <div style="border: 1px solid red;">
        'image-leopard-assets.jpg' = {{ 'image-leopard-assets.jpg' }}
        <br>
        'image-leopard-assets.jpg' | asset_url = {{ 'image-leopard-assets.jpg' | asset_url }}
        <br>
        'image-leopard-assets.jpg' | asset_img_url: 'master'= {{ 'image-leopard-assets.jpg' | asset_img_url: 'master' }}
        <br>
        'image-leopard-assets.jpg' | asset_img_url: '200x' = {{ 'image-leopard-assets.jpg' | asset_img_url: '200x' }}
        <br>
          <img src="{{  'image-leopard-assets.jpg' | asset_img_url: 'small'}}">
        <br>
        {{ 'image-leopard-assets.jpg' | asset_img_url: '80' | img_tag }}
      </div>
    </div>
    <div id="get_image_v_4">
      <h2>get_image_v_4 - assets</h2>
      <div style="border: 1px solid red;">
        'https://cdn.shopify.com/s/files/1/0701/3154/8434/files/image-leopard-files.jpg?v=1674905566' = {{ 'https://cdn.shopify.com/s/files/1/0701/3154/8434/files/image-leopard-files.jpg?v=1674905566' }}
        <br>
        <img src="https://cdn.shopify.com/s/files/1/0701/3154/8434/files/image-leopard-files.jpg?v=1674905566" alt="">
        <br>
        {{ 'https://cdn.shopify.com/s/files/1/0701/3154/8434/files/image-leopard-files.jpg' | img_tag }}
        <br>
      </div>
    </div>
    <div id="get_image_v_5">
      <h2>get_image_v_5 - assets</h2>
      <div style="border: 1px solid purple;">
        <div id="get_image_v_5_1" style="border: 1px solid lightblue;">
