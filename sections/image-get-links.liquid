        </div>
        <div id="get_image_v_5_2" style="border: 1px solid darkblue;">
          'shopify-icon.svg' | asset_url =  {{ 'shopify-icon.svg' | asset_url }}
          <br>
          'shopify-icon.svg' | asset_img_url: '200x' =  {{ 'shopify-icon.svg' | asset_img_url: '200x' | asset_url }}
          <br>
          <img src="{{ 'shopify-icon.svg' | asset_url }}" alt="">
          <br>
          <img src="{{ 'shopify-icon.svg' | asset_img_url: '50x' }}" alt="">
          <br>
          {{ 'shopify-icon.svg'  | asset_url  | img_tag }}          
        </div>

        <div id="get_image_v_5_3" style="border: 1px solid pink;">
          <br>
          new <br>
          <div class="svg-icon" style="">
            {% render 'icon-shopify-snippets' %}
          </div>
          <style>
            .svg-icon {
              height: 56px;
              width: 44px;
              display: flex;
              align-items: center;
              justify-content: center;
            }
            .svg-icon svg{
              height: inherit;
              width: inherit;
            }
          </style>
          <br>
          <br>
          old 
          <br>
          {% include 'icon-shopify-snippets' %}
          <br>     
          <br>                   
        </div>
      </div>
    </div>
    </div>
{% schema %}
{
  "name": "Image get Links",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "image_picker",
      "id": "get_image_v_1",
      "label": "Get Image V1"
    },
    {
      "type": "image_picker",
      "id": "get_image_v_2",
      "label": "Image_picker without image"
    }
  ],
  "presets": [
    {
      "name": "Image get Links"
    }
  ]
}
{% endschema %}
